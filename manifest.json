{
   "name": "chrome-vue-extension",
   "version": "1.0",
   "manifest_version": 3,
   "action": {
       "default_popup": "index.html"
   },
   "permissions": [
       "activeTab"
   ],
   "background": {
       "service_worker": "background/background.js"
   },
   "content_scripts": [
       {
       "matches": ["<all_urls>"],
       "js": ["main.js"]
       }
   ]
}